  <table class="table table-light  table-responsive" style="font-size: 79%;">
    <thead class="thead-light">
        <tr>
            <th>id usuario: </th>
            <th>nombres: </th> 
            <th>fecha de nacimiento: </th>
            <th>numero de telefono: </th>
            <th>correo: </th>
            <th>seguro social: </th>
            <th>ciudad donde vive: </th>
            <th>direccion: </th>
            <th>apt-suite-unidad: </th>
            <th>codigo postal: </th>
            <th>numero de referido: </th>
            <th>nombre de banco: </th>
            <th>numero de cuenta: </th>
            <th>numero de ruta: </th>
            <th>nombre de beneficirario: </th>
            <th>licencia de conduccion: </th>
        </tr>
        </thead>
        <tbody>
          <% results.forEach((user)=>{ %>
          <tr> 
              <td> <%=user.id_usuario  %>  </td>
              <td> <%=user.nombres  %> </td>
              <td> <%=user.apellidos  %>  </td>
              <td> <%=user.fecha_nacimiento  %>  </td>
              <td> <%=user.numero_telefono  %>  </td>
              <td> <%=user.correo  %>  </td>
              <td> <%=user.seguro_social  %>  </td>
              <td> <%=user.ciudad_donde_vive  %>  </td>
              <td> <%=user.direccion  %>  </td>
              <td> <%=user.apt_suite_unidad  %>  </td>
              <td> <%=user.codigo_postal  %>  </td>
              <td> <%=user.numero_referido  %>  </td>
              <td> <%=user.nombre_banco  %>  </td>
              <td> <%=user.numero_de_cuenta  %>  </td>
              <td> <%=user.numero_de_ruta  %>  </td>
              <td> <%=user.nombre_beneficiario  %>  </td>
              <td> <%=user.licencia_conduccion  %>  </td>
   
         </tr> 

        <% }) %>
        </tbody>


</div>

     


  <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js" ></script>

